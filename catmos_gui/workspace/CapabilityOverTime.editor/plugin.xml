<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="CapOverTime.presentation.CapOverTimeModelWizardID"
            name="%_UI_CapOverTimeModelWizard_label"
            class="CapOverTime.presentation.CapOverTimeModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CapOverTimeModelFile.gif">
         <description>%_UI_CapOverTimeModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="CapOverTime.presentation.CapOverTimeEditorID"
            name="%_UI_CapOverTimeEditor_label"
            icon="icons/full/obj16/CapOverTimeModelFile.gif"
            extensions="capovertime"
            class="CapOverTime.presentation.CapOverTimeEditor"
            contributorClass="CapOverTime.presentation.CapOverTimeActionBarContributor">
      </editor>
   </extension>

</plugin>
