        href="SimpleScenario.gmftool#//@palette/@tools.1/@tools.1"/>
    <diagramLink
        href="SimpleScenario.gmfgraph#CapabilitySatisfies"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="SimpleScenario.ecore#//Capability/satisfies"/>
  </links>
  <links>
    <labelMappings
        xsi:type="gmfmap:DesignLabelMapping"
        readOnly="true">
      <diagramLabel
          href="SimpleScenario.gmfgraph#CapabilityUpgradeUpgradingExternalLabel"/>
    </labelMappings>
    <tool
        xsi:type="gmftool:CreationTool"
        href="SimpleScenario.gmftool#//@palette/@tools.1/@tools.2"/>
    <diagramLink
        href="SimpleScenario.gmfgraph#CapabilityUpgradeUpgrading"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="SimpleScenario.ecore#//CapabilityUpgrade/upgrading"/>
  </links>
  <diagram>
    <diagramCanvas
        href="SimpleScenario.gmfgraph#SimpleScenario"/>
    <domainModel
        href="SimpleScenario.ecore#/"/>
    <domainMetaElement
        href="SimpleScenario.ecore#//Scenario"/>
    <palette
        href="SimpleScenario.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
